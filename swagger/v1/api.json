  "paths": {
    "/api/v1/groups": {
      "get": {
        "summary": "Lister les groupes (représentation des territoires)",
        "tags": [
          "Group"
        ],
        "operationId": "getGroups",
        "description": "Renvoie tous les groupes, qui représentent les territoires, de manière paginée.",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "La page souhaitée",
            "example": "1",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "per",
            "in": "query",
            "description": "Le nombre d'éléments souhaités par page",
            "example": "10",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Retourne des Groups",
            "content": {
              "application/json": {
                "examples": {
                  "example": {
                    "value": {
                      "groups": [
                        {
                          "id": 69,
                          "label": "Territoire n°3",
                          "name": "3"
                        },
                        {
                          "id": 70,
                          "label": "Territoire n°4",
                          "name": "4"
                        }
                      ],
                      "meta": {
                        "current_page": 2,
                        "next_page": 3,
                        "prev_page": 1,
                        "total_pages": 3,
                        "total_count": 5
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
